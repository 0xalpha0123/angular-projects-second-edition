{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "tour",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  },
  "projects": {
    "tour": {
      "projectType": "application",
      "root": "apps/tour",
      "sourceRoot": "apps/tour/src",
      "prefix": "packt",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/tour",
            "index": "apps/tour/src/index.html",
            "main": "apps/tour/src/main.ts",
            "polyfills": "apps/tour/src/polyfills.ts",
            "tsConfig": "apps/tour/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/tour/src/favicon.ico", "apps/tour/src/assets"],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "apps/tour/src/styles.css"
            ],
            "scripts": [],
            "allowedCommonJsDependencies": ["chart.js"]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/tour/src/environments/environment.ts",
                  "with": "apps/tour/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "tour:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "tour:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "tour:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/tour/src/**/*.ts",
              "apps/tour/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/tour"],
          "options": {
            "jestConfig": "apps/tour/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "tour-e2e": {
      "root": "apps/tour-e2e",
      "sourceRoot": "apps/tour-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/tour-e2e/cypress.json",
            "tsConfig": "apps/tour-e2e/tsconfig.e2e.json",
            "devServerTarget": "tour:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "tour:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/tour-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "visitor": {
      "projectType": "library",
      "root": "libs/visitor",
      "sourceRoot": "libs/visitor/src",
      "prefix": "packt",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/visitor/src/**/*.ts",
              "libs/visitor/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/visitor"],
          "options": {
            "jestConfig": "libs/visitor/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "admin": {
      "projectType": "library",
      "root": "libs/admin",
      "sourceRoot": "libs/admin/src",
      "prefix": "packt",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/admin/src/**/*.ts",
              "libs/admin/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/admin"],
          "options": {
            "jestConfig": "libs/admin/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "poi": {
      "projectType": "library",
      "root": "libs/poi",
      "sourceRoot": "libs/poi/src",
      "prefix": "packt",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/poi/src/**/*.ts",
              "libs/poi/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/poi"],
          "options": {
            "jestConfig": "libs/poi/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
